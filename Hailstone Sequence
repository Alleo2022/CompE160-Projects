#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main(void) {

int userNum;
int count;

scanf ("%d", &userNum);

while (userNum != 1 ) {
  if (userNum%2 ==0){
	printf ("%d\t", userNum);
	userNum /= 2;
}
else {
	printf ("%d\t", userNum);
	userNum = userNum*3 +1;
}
++count;
if (count % 10 == 0){
	printf ("\n");
	count = 0;
 }
}
printf ("1");
return (0);
}
